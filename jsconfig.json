{
    "compilerOptions": {
        "checkJs": true,
        "jsx": "preserve",
        "target": "es6",
        "module": "commonjs",
        "baseUrl": "./src",
        "paths": {
            "components/*": ["components/*"],
            "services/*": ["services/*"],
            "assets/*": ["assets/*"],
            "actions/*": ["actions/*"]
